
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<dashboard-app></dashboard-app>

<footer>
    <a href="@Url.Action("Attributions")" target="_new">Attributions</a>
</footer>
@section Scripts {
    <!--Script references. -->
    @Scripts.Render("~/bundles/cookies")
    <!--Reference the SignalR library. -->
    @Scripts.Render("~/Scripts/jquery.signalR-2.2.0.js")
    <!--Reference the autogenerated SignalR hub script. -->
    @Scripts.Render("~/signalr/hubs")

    @Scripts.Render("~/Scripts/angular2/shim.min.js")
    @Scripts.Render("~/Scripts/angular2/zone.js")
    @Scripts.Render("~/Scripts/angular2/Reflect.js")
    @Scripts.Render("~/Scripts/angular2/Rx.min.js")
    @Scripts.Render("~/Scripts/angular2/core.umd.min.js")
    @Scripts.Render("~/Scripts/angular2/common.umd.min.js")
    @Scripts.Render("~/Scripts/angular2/compiler.umd.min.js")
    @Scripts.Render("~/Scripts/angular2/platform-browser.umd.min.js")
    @Scripts.Render("~/Scripts/angular2/platform-browser-dynamic.umd.min.js")
    )
    @Scripts.Render("~/bundles/app")

}