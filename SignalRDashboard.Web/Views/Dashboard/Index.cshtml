
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<dashboard-app></dashboard-app>

<footer>
    <a href="@Url.Action("Attributions")" target="_new">Attributions</a>
</footer>
@section Scripts {
    <!--Script references. -->
    <!--Reference the SignalR library. -->
    @Scripts.Render("~/Scripts/jquery.signalR-2.2.0.js")
    <!--Reference the autogenerated SignalR hub script. -->
    @Scripts.Render("~/signalr/hubs")

    <!-- IE required polyfill -->
    @Scripts.Render("~/Scripts/angular2-beta0/es6-shim.min.js")
    @Scripts.Render("~/Scripts/angular2-beta0/angular2-polyfills.js")
    @Scripts.Render("~/Scripts/angular2-beta0/Rx.umd.js")
    @Scripts.Render("~/Scripts/angular2-beta0/angular2-all.umd.dev.js")

    @Scripts.Render("~/bundles/app")
    
}